def largest(arr, n):
    
    max_element = arr[0]
 

def largest(arr, n):

    max_element = arr[0]
 

    for i in range(1, n):
        if arr[i] > max_element:
            max_element = arr[i]
    return max_element
 

n = int(input("Enter the number of elements in the array: "))
arr = []
for i in range(n):
    element = int(input("Enter element " + str(i+1) + ": "))
    arr.append(element)

largest_element = largest(arr, n)
square_of_largest = largest_element ** 2
print("Square of the largest element in the given array:", square_of_largest)